const Discord = module.require("discord.js");
const fs = require("fs");
module.exports.run = async (bot,message,args) => {
    
    let enbed = new Discord.RichEmbed()
    .setDescription("Clans")
    .setColor('#4B0082')
    .addField("🎏 YourClan", )

